<?xml version="1.0" encoding="UTF-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" android:layout_height="wrap_content" android:layout_width="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout android:gravity="start"
        android:id="@+id/linearCommunity"
        android:layout_height="match_parent"
        android:paddingHorizontal="15dp"
        android:paddingVertical="10dp"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">

        <TextView
            android:id="@+id/txtStories"
            style="@style/txtUrbanistromanbold24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="272dp"
            android:layout_marginTop="2dp"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="@string/lbl_community"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="@string/lbl_community" />


        <LinearLayout
            android:id="@+id/CommunityPost1"
            style="@style/groupStyleyellow_900cornerRadius"
            android:layout_width="match_parent"
            android:layout_height="139dp"
            android:layout_marginTop="24dp"
            android:gravity="start"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtStories">

            <TextView
                android:id="@+id/cTitle1"
                style="@style/txtUrbanistromanmedium14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@string/lbl_rizal_reynaldhi"
                tools:text="@string/lbl_rizal_reynaldhi" />

            <TextView
                android:id="@+id/cDescription1"
                style="@style/txtUrbanistromanregular13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:text="@string/msg_most_people_nev"
                tools:text="@string/msg_most_people_nev" />

            <LinearLayout
                android:id="@+id/cMember1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginBottom="@dimen/_9pxv"
                android:gravity="start"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageVector"
                    android:layout_width="@dimen/_9pxh"
                    android:layout_height="@dimen/_9pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_vector" />

                <TextView
                    android:id="@+id/txtEightHundred"
                    style="@style/txtInterregular9"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_weight="0.65"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="429"
                    tools:text="429" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/CommunityPost2"
            style="@style/groupStyleyellow_900cornerRadius"
            android:layout_width="match_parent"
            android:layout_height="139dp"
            android:layout_marginTop="20dp"
            android:gravity="start"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/CommunityPost1">

            <TextView
                android:id="@+id/cTitle2"
                style="@style/txtUrbanistromanmedium14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@string/lbl_rizal_reynaldhi"
                tools:text="@string/lbl_rizal_reynaldhi" />

            <TextView
                android:id="@+id/cDescription2"
                style="@style/txtUrbanistromanregular13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:text="@string/msg_most_people_nev"
                tools:text="@string/msg_most_people_nev" />

            <LinearLayout
                android:id="@+id/cMember2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginBottom="@dimen/_9pxv"
                android:gravity="start"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageVector2"
                    android:layout_width="@dimen/_9pxh"
                    android:layout_height="@dimen/_9pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_vector" />

                <TextView
                    android:id="@+id/txtEightHundred2"
                    style="@style/txtInterregular9"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_weight="0.65"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="878"
                    tools:text="878" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/CommunityPost3"
            style="@style/groupStyleyellow_900cornerRadius"
            android:layout_width="match_parent"
            android:layout_height="139dp"
            android:layout_marginTop="24dp"
            android:gravity="start"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/CommunityPost2">

            <TextView
                android:id="@+id/cTitle3"
                style="@style/txtUrbanistromanmedium14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@string/lbl_rizal_reynaldhi"
                tools:text="@string/lbl_rizal_reynaldhi" />

            <TextView
                android:id="@+id/cDescription3"
                style="@style/txtUrbanistromanregular13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:text="@string/msg_most_people_nev"
                tools:text="@string/msg_most_people_nev" />

            <LinearLayout
                android:id="@+id/linearCommentspost3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginBottom="@dimen/_9pxv"
                android:gravity="start"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageVector3"
                    android:layout_width="@dimen/_9pxh"
                    android:layout_height="@dimen/_9pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_vector" />

                <TextView
                    android:id="@+id/txtEightHundred3"
                    style="@style/txtInterregular9"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_weight="0.65"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="185"
                    tools:text="185" />
            </LinearLayout>
        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>
